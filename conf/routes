GET     /api/tasks                              controllers.TodoTaskController.getTodoTaskList
PUT     /api/tasks/complete/:taskID             controllers.TodoTaskController.completeTask(taskID: String)
PUT     /api/tasks/uncomplete/:taskID           controllers.TodoTaskController.uncompleteTask(taskID: String)
POST    /api/tasks/create                       controllers.TodoTaskController.createTask
DELETE  /api/tasks/:taskID                      controllers.TodoTaskController.deleteTask(taskID: String)